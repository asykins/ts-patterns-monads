{
	"compilerOptions": {
	  "target": "es5",
	  "module": "commonjs",
	  "lib": [
		"es6",
		"DOM"
	  ],
	  "allowJs": true,
	  "outDir": "dist",
	  "rootDir": "src",
	  "strict": true,
	  "noImplicitAny": true,
	  "esModuleInterop": true,
	  "resolveJsonModule": true,
	  "inlineSourceMap": true
	},
	"include": [
		"./src/**/*.ts"
	]
  }